{
  "name": "rockman",
  "private": true,
  "description": "SPITZ草野マサムネ ロック大陸漫遊記のプレイリスト集",
  "version": "1.0.0",
  "packageManager": "yarn@3.2.0",
  "author": "cieloazul310 <cieloazul310@gmail.com>",
  "keywords": [
    "gatsby",
    "typescript",
    "material-ui"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "develop": "gatsby develop -o -H 0.0.0.0",
    "serve": "gatsby serve -o -H 0.0.0.0 --prefix-paths",
    "clean": "gatsby clean",
    "deploy": "gh-pages -d public",
    "lint": "eslint src/ --fix --ignore-pattern \"**/stories/**/*\" && eslint gatsby/ --fix",
    "format": "prettier \"{src,gatsby}/**/*.{js,jsx,ts,tsx}\" --parser typescript --write",
    "test": "jest",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@cieloazul310/gatsby-theme-aoi": "^3.1.0",
    "@cieloazul310/gatsby-theme-aoi-blog-components": "^3.1.0",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.11",
    "d3-dsv": "^3.0.1",
    "gatsby": "^5.7.0",
    "gatsby-plugin-canonical-urls": "^5.7.0",
    "gatsby-plugin-eslint": "^4.0.3",
    "gatsby-plugin-google-gtag": "5.7.0",
    "gatsby-plugin-manifest": "^5.7.0",
    "gatsby-plugin-sitemap": "^6.7.0",
    "gatsby-source-filesystem": "^5.7.0",
    "gatsby-transformer-yaml": "^5.7.0",
    "iso-3166-1": "^2.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "^9.4.3",
    "react-swipeable-views": "^0.14.0",
    "react-swipeable-views-utils": "^0.14.0",
    "react-virtualized": "^9.22.3",
    "react-window": "^1.8.8",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-typescript": "^7.21.0",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/core-common": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.6",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-swipeable-views": "^0.13.1",
    "@types/react-swipeable-views-utils": "^0.13.4",
    "@types/react-virtualized": "^9.21.21",
    "@types/react-window": "^1.8.5",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "babel-jest": "^29.4.3",
    "babel-preset-gatsby": "^3.7.0",
    "babel-preset-react-app": "^10.0.1",
    "eslint": "^8.35.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^4.0.0",
    "gh-pages": "^5.0.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.4.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "react-test-renderer": "^18.2.0",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix --ignore-pattern \"**/stories/**/*\"",
      "prettier --parser typescript --write"
    ],
    "./gatsby/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --parser typescript --write"
    ]
  }
}
