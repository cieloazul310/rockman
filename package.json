{
  "name": "rockman",
  "private": true,
  "description": "SPITZ草野マサムネ ロック大陸漫遊記のプレイリスト集",
  "version": "1.0.0",
  "author": "cieloazul310 <cieloazul310@gmail.com>",
  "keywords": [
    "gatsby",
    "typescript",
    "material-ui"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "develop": "gatsby develop -o -H 0.0.0.0",
    "serve": "gatsby serve -o -H 0.0.0.0 --prefix-paths",
    "clean": "gatsby clean",
    "deploy": "gh-pages -d public",
    "lint": "eslint src/ --fix --ignore-pattern \"**/stories/**/*\" && eslint gatsby/ --fix",
    "format": "prettier \"{src,gatsby}/**/*.{js,jsx,ts,tsx}\" --parser typescript --write",
    "test": "jest",
    "prepare": "husky",
    "storybook": "start-storybook -p 6006",
    "lint-staged": "lint-staged"
  },
  "engines": {
    "node": "20"
  },
  "dependencies": {
    "@cieloazul310/gatsby-theme-aoi": "^3.3.0",
    "@cieloazul310/gatsby-theme-aoi-blog-components": "^3.3.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^5.16.13",
    "@mui/material": "^5.16.13",
    "d3-dsv": "^3.0.1",
    "gatsby": "^5.14.1",
    "gatsby-plugin-canonical-urls": "^5.14.0",
    "gatsby-plugin-eslint": "^4.0.4",
    "gatsby-plugin-google-gtag": "^5.14.0",
    "gatsby-plugin-manifest": "^5.14.0",
    "gatsby-plugin-sitemap": "^6.14.0",
    "gatsby-source-filesystem": "^5.14.0",
    "gatsby-transformer-yaml": "^5.14.0",
    "iso-3166-1": "^2.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-intersection-observer": "^9.14.0",
    "react-swipeable-views": "^0.14.0",
    "react-swipeable-views-utils": "^0.14.0",
    "react-virtualized": "^9.22.5",
    "react-window": "^1.8.11",
    "yaml": "^2.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.0",
    "@jest/globals": "^29.7.0",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/core-common": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.8.3",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/react-swipeable-views": "^0.13.6",
    "@types/react-swipeable-views-utils": "^0.13.7",
    "@types/react-virtualized": "^9.22.0",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-remove-graphql-queries": "^5.12.0",
    "babel-preset-gatsby": "^3.12.0",
    "babel-preset-react-app": "^10.0.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.10.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-webpack-plugin": "^4.2.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2",
    "react-docgen-typescript-plugin": "^1.0.8",
    "react-test-renderer": "^18.3.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix --ignore-pattern \"**/stories/**/*\"",
      "prettier --parser typescript --write"
    ],
    "./gatsby/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --parser typescript --write"
    ],
    "./*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --parser typescript --write"
    ]
  }
}